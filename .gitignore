# ==========================
# JM-MD BOT .gitignore
# ==========================

# Node modules (do not push huge dependencies)
node_modules/

# Downloads folder (big media files)
downloads/

# Auth/session files (sensitive secrets!)
auth/

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Environment files (keep your API keys safe)
.env

# Mac system files
.DS_Store

# Termux specific
.termux/

# Build artifacts
dist/
build/

# Optional: package lock (if you want deterministic installs, keep it tracked)
# package-lock.json

# IDE/editor configs
.vscode/
.idea/
*.sublime-workspace
*.sublime-project
